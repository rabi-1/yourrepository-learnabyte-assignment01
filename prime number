#include<stdio.h>
int main()
{
	int n,i,flag=0;
	printf("enter the number to check prime");
	scanf("%d",&n);
	for(i=2;i<=n/2;i++)
	{
		if(n%i==0)
	     printf("number is not a prime number");
	     flag=1;
		for(i=1;i<=n;i++)
		 	{
		 		if(n%i==0)
		 		printf("%d",i);
			 }
		
    }
    if(flag==0)
    {
    	printf("number is prime");
    	return 0;
	}
	
}
